-- Get the current game's PlaceId
local PlaceId = game.PlaceId

-- Execute the script if the game is supported
if SupportedGames[PlaceId] then
    print("Supported game detected! Executing script...")
    SupportedGames[PlaceId]()
else
    warn("This game is not supported, join the discord.gg/2xrscripts for supprt!")
end
